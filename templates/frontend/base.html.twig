<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <title>{% block title %}MapaÚtulků.cz{% endblock %}</title>

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/colors/main.css') }}" id="colors">
    {% endblock %}
</head>

<body>

<div id="wrapper">
    {% include 'frontend/navigation/top.html.twig' %}

    {% block flashes %}{% include 'frontend/_fragment/flashes.html.twig' %}{% endblock %}

    {% block body %}{% endblock %}

    {% block footer %}
        {% include 'frontend/footer/_footer.html.twig' %}
    {% endblock %}

</div>

    {% block javascripts %}
        <script type="text/javascript" src="{{ asset('assets/scripts/vendor/jquery-2.2.0.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('assets/scripts/vendor/mmenu.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('assets/scripts/vendor/chosen.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('assets/scripts/theme.js') }}"></script>
        <script type="text/javascript" src="{{ asset('assets/scripts/application.js') }}"></script>
    {% endblock %}

</body>
</html>
